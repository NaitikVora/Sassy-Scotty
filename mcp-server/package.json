{
  "name": "sassy-scotty-mcp",
  "version": "1.0.0",
  "description": "MCP server for Sassy Scotty - CMU student productivity app",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:http": "tsx watch src/httpServer.ts",
    "build": "tsc",
    "start": "node dist/httpServer.js",
    "start:http": "node dist/httpServer.js",
    "start:mcp": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "canvas",
    "cmu",
    "productivity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "node-ical": "^0.22.1",
    "playwright": "^1.48.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
